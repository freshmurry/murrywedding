$('#photos').html("<%= j render 'photos_list' %>")
<% if @pin.photos.blank? %>
  $('#photo_check').hide();
<% end %>

<% if !@pin.listing_name.blank? && !@pin.photos.blank? && !@pin.location_type.blank? %>
    $('#publish_button').attr('disabled', false);
<% else %>
    $('#publish_button').attr('disabled', true);
<% end %>